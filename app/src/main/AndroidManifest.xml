<?xml version="1.0" encoding="utf-8"?>
<manifest package="ahat.mmsnap"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/> <!-- the following permission are required for notifications and internet communication -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TestingActivity"
            android:label="@string/title_activity_testing"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".AchievementsActivity"
            android:label="@string/title_activity_achievements"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".DailyEvaluationsDetailActivity"
            android:label="@string/title_activity_daily_evaluations_detail"
            android:parentActivityName=".DailyEvaluationsListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.DailyEvaluationsListActivity"/>
        </activity>
        <activity
            android:name=".DailyEvaluationsListActivity"
            android:label="@string/title_activity_daily_evaluations_list"
            android:parentActivityName=".IfThenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.IfThenActivity"/>
        </activity>
        <activity
            android:name=".WeeklyEvaluationsListActivity"
            android:label="@string/title_activity_weekly_evaluations_list"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login">
        </activity>
        <activity
            android:name=".AgentsActivity"
            android:label="@string/title_activity_agents"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".CopingPlansDetailActivity"
            android:label="@string/title_activity_coping_plans_detail"
            android:parentActivityName=".CopingPlansActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.CopingPlansActivity"/>
        </activity>
        <activity
            android:name=".ActionPlansDetailActivity"
            android:label="@string/title_activity_action_plans_detail"
            android:parentActivityName=".ActionPlansActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.ActionPlansActivity"/>
        </activity>
        <activity
            android:name=".CounterfactualDetailActivity"
            android:label="@string/title_activity_counterfactual_detail"
            android:parentActivityName=".IfThenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.IfThenActivity"/>
        </activity>
        <activity
            android:name=".CopingPlansActivity"
            android:label="@string/title_activity_coping_plans"
            android:parentActivityName=".IfThenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.IfThenActivity"/>
        </activity>
        <activity
            android:name=".ActionPlansActivity"
            android:label="@string/title_activity_action_plans"
            android:parentActivityName=".IfThenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.IfThenActivity"/>
        </activity>
        <activity
            android:name=".IfThenActivity"
            android:label="@string/title_activity_if_then"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test"
            android:parentActivityName=".EduActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.EduActivity"/>
        </activity>
        <activity
            android:name=".EduActivity"
            android:label="@string/title_activity_edu"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".EQVASActivity"
            android:label="@string/title_activity_eqvas"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".WeeklyEvaluationActivity"
            android:label="@string/title_activity_weekly_evaluation"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".SelfRatedHealthActivity"
            android:label="@string/title_activity_self_rated_health"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".IntentionsAndPlansActivity"
            android:label="@string/title_activity_plans"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".HealthRiskActivity"
            android:label="@string/title_activity_health_risk"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".EfficacyActivity"
            android:label="@string/title_activity_efficacy"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.AssessmentsActivity"/>
        </activity>
        <activity
            android:name=".AssessmentsActivity"
            android:label="@string/title_activity_assessments"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".MMSNAPSubCategoryActivity"
            android:parentActivityName=".MMSNAPActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MMSNAPActivity"/>
        </activity>
        <activity
            android:name=".MMSNAPActivity"
            android:label="@string/title_activity_mmsnap"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahat.mmsnap.MainActivity"/>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service
            android:name=".Notifications.PendingEvaluationNotificationService"
            android:enabled="true"
            android:exported="false"/>
        <service
            android:name=".Notifications.ReminderNotificationService"
            android:enabled="true"
            android:exported="false"/>

        <receiver android:name=".Notifications.PendingEvaluationsAlarmReceiver"/>
        <receiver android:name=".Notifications.ReminderAlarmReceiver"/>
        <receiver android:name=".Notifications.PendingEvaluationNotificationServiceStarterReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="android.intent.action.TIMEZONE_CHANGED"/>
                <action android:name="android.intent.action.TIME_SET"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".Notifications.ReminderNotificationServiceStarterReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="android.intent.action.TIMEZONE_CHANGED"/>
                <action android:name="android.intent.action.TIME_SET"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>